class Fixnum
{
  func to_bool()
  {
    return self != 0;
  }
  
  func times()
  {
    var i = 0;
    while (i < self)
    {
      yield(i);
      i = i + 1;
    }
  }

  func map() {
    var i = 0;
    var result = @[];
    while (i < self) {
      var val = yield(i);
      result.insert(val);
      i = i + 1;
    }
    return result;
  }
}
