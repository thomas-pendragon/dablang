## CODE

W_HEADER 2
W_SECTION DATA, "data"
W_SECTION CODE, "code"
W_SECTION SYMBOLS, "symb"
W_SECTION FUNC, "func"
W_END_HEADER

DATA:
  W_STRING "___"

STR_DATA:
  W_STRING "test: allocated string\n"

SYM_MAIN:
  W_STRING "main"

SYM_NEW:
  W_STRING "new"

SYMBOLS:
  W_SYMBOL SYM_MAIN
  W_SYMBOL SYM_NEW 

CODE:
  NOP

Ffoo:
STACK_RESERVE 0
LOAD_STRING R0, STR_DATA, 23
LOAD_CLASS R1, 1
Q_SET_INSTCALL R0, R1, S1, R0
Q_SET_SYSCALL RNIL, 0, R0
Q_RETURN RNIL

FUNC:
  W_METHOD 0, -1, Ffoo

## OPTIONS

--noraw --noautorelease

## TESTCASE

$leaktest

## EXPECT

test: allocated string
leaktest: no leaks

