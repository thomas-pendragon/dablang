## CODE

W_HEADER 3
W_SECTION DATA, "data"
W_SECTION CODE, "code"
W_SECTION SYMBOLS, "symb"
W_SECTION FUNC, "fext"
W_END_HEADER

DATA:
  W_STRING "___"

STR_DATA:
  W_STRING "test: allocated string\n"

SYM_MAIN:
  W_STRING "main"

SYM_NEW:
  W_STRING "new"

SYMBOLS:
  W_SYMBOL SYM_MAIN
  W_SYMBOL SYM_NEW 

CODE:
  NOP

Ffoo:
STACK_RESERVE 0
LOAD_STRING R4, STR_DATA, 23
LOAD_CLASS R1, 1
INSTCALL R0, R1, S1, R4
SYSCALL RNIL, 0, R0
RELEASE R4
RELEASE R0
RETURN RNIL

FUNC:
  W_METHOD 0, -1, Ffoo, 0, 0
  W_METHOD_ARG 0, 0

## OPTIONS

--noraw --noautorelease

## TESTCASE

$leaktest

## EXPECT

test: allocated string
leaktest: no leaks

