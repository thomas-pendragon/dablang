## CODE

  W_HEADER 3
  W_SECTION DATA, "data"
  W_SECTION SYMBOLS, "symb"
  W_SECTION CODE, "code"
  W_SECTION FUNC, "func"
  W_SECTION CLASS, "clas"
  W_END_HEADER

DATA:
  W_STRING "notcount"

DATA2:
  W_STRING "foo"
  W_STRING "main"

DATA3:
  W_STRING "Foobar"

CLASS:
  W_CLASS 500, 0, 4 # Foobar

SYMBOLS:
  W_SYMBOL DATA + 0 # notcount
  W_SYMBOL DATA + 3 # count
  W_SYMBOL DATA2 + 0 # foo
  W_SYMBOL DATA2 + 4 # main
  W_SYMBOL DATA3 # Foobar

CODE:
  NOP

Fmain:
  LOAD_CLASS R2, 0
  SYSCALL RNIL, 0, R2
  LOAD_CLASS R2, 500
  SYSCALL RNIL, 0, R2
  RETURN RNIL

FUNC:
  W_METHOD 3, -1, Fmain

## OPTIONS

--noraw --newformat

## TESTCASE

$output

## EXPECT

ObjectFoobar
