## CODE

func foo(a, b)
{
  if (a)
  {
    if (b)
    {
      print("a && b");
    }
    else
    {
      print("a && !b");
    };
  }
  else
  {
    if (b)
    {
      print("!a && b");    
    }
    else
    {
      print("!a && !b");
    };
  };
}

