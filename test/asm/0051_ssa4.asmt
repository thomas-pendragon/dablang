## CODE

func foo() { return true; }

func main()
{
  var a = 1;
  a = 2;
  var b = a;
  a = 7;
  if (foo())
  {
    a = 8;
  }
  else
  {
    a = 1;
  }
  print(a);
  var c = 8;
  c = b;
  print(c);
}

