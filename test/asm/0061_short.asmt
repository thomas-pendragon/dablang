## CODE

func fun1()
{
  print("fun1");
  return false;
}

func fun2()
{
  print("fun2");
  return true;
}

func main_and()
{
  print(fun1() && fun2());
}

func main_or()
{
  print(fun1() || fun2());
}

func main()
{
  main_and();
  main_or();
}

