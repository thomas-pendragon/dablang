## ASM CODE

W_HEADER 2
W_SECTION _DATA, "data"
W_SECTION _SYMB, "symb"
W_SECTION _CODE, "code"
W_SECTION _FUNC, "func"

W_END_HEADER

_DATA:
  W_STRING "hello world"

SYM_MAIN:
  W_STRING "main"

_SYMB:
  W_SYMBOL SYM_MAIN

_CODE:
  NOP

Fmain:
  STACK_RESERVE 0
  Q_RETURN RNIL 

_FUNC:
  W_METHOD 0, -1, Fmain

