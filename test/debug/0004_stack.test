## ASM CODE

W_HEADER 3
W_SECTION _DATA, "data"
W_SECTION _SYMB, "symb"
W_SECTION _CODE, "code"
W_SECTION _FUNC, "fext"

W_END_HEADER

_DATA:
  W_STRING "hello world"

SYM_MAIN:
  W_STRING "main"

_SYMB:
  W_SYMBOL SYM_MAIN

_CODE:
  NOP

Fmain:
  STACK_RESERVE 0
  RETURN RNIL 

_FUNC:
  W_METHOD 0, -1, Fmain, 0, 0
  W_METHOD_ARG 0, 0

