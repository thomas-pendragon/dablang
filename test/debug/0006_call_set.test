## ASM CODE

W_HEADER 2
W_SECTION _DATA, "data"
W_SECTION _SYMB, "symb"
W_SECTION _CODE, "code"
W_SECTION _FUNC, "func"

W_END_HEADER

_DATA:
  W_STRING "hello world"

SYM_MAIN:
  W_STRING "main"

SYM_FUN1:
  W_STRING "fun1"

_SYMB:
  W_SYMBOL SYM_FUN1
  W_SYMBOL SYM_MAIN

_CODE:
  NOP

                   Ffun1:
                                 STACK_RESERVE 0
                                 LOAD_TRUE R0
                                 RETURN R0
 
                   Fmain:
                                 STACK_RESERVE 0
                                 CALL R7, S0
                                 SYSCALL RNIL, 0, R7
                                 RETURN RNIL

_FUNC:
  W_METHOD 0, -1, Ffun1
  W_METHOD 1, -1, Fmain

