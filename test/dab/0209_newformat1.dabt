## CODE

class Zomg
{
  func foo(a, b)
  {
    print(a);
    print(" = ");
    print(b);
    print("; ");
  }  
}

func main()
{
  var i = 4;
  while (i > 0)
  {
    var zomg = Zomg.new;
    zomg.foo("foobar", i);
    i = i - 1;    
  }
}

