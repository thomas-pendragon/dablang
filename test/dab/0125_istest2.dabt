## CODE

class TestQ
{
};

class TestW : TestQ
{
};

func test(object)
{
  print(object);
  print(" (");
  print(object.class);
  print(") is a:");
  if (object is String)
  {
    print(" String");
  }
  if (object is Fixnum)
  {
    print(" Fixnum");
  }
  if (object is TestQ)
  {
    print(" TestQ");
  }
  if (object is Object)
  {
    print(" Object");
  }
  print("\n");
}

func main()
{
  test("123");
  test(String.new("foobar"));
  test(nil);
  test(true);
  test(false);
  test(123);
  test(Fixnum.new(345));
  test(@[1,2,3]);
  test(TestQ.new);
  test(TestW.new);
}

## OPTIONS

--no-opt

## RUN

main

## EXPECT OK

123 (LiteralString) is a: String Object
foobar (String) is a: String Object
nil (NilClass) is a: Object
true (Boolean) is a: Object
false (Boolean) is a: Object
123 (Fixnum) is a: Fixnum Object
345 (Fixnum) is a: Fixnum Object
[1, 2, 3] (Array) is a: Object
#TestQ (TestQ) is a: TestQ Object
#TestW (TestW) is a: TestQ Object

