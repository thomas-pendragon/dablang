func import_ffi(method, name = nil)
{
  if (name == nil)
  {
    name = method.name;
  }
  __warn("Import FFI method '" + name + "' as '" + method.to_s + "'\n");
  __dlimport("bin/libcffitest.dylib", method, name);
}

[import_ffi("ffi_simple_test")]
func FFIsimpleTest<Int32>() {}

func main()
{
  print(FFIsimpleTest());
}

